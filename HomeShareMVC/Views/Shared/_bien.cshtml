@model HomeShare.DAL.BienEchange

<div class="sl-slide" data-orientation="horizontal" data-slice1-rotation="-25" data-slice2-rotation="-25" data-slice1-scale="2" data-slice2-scale="2">
    <div class="sl-slide-inner">
        <div class="bg-img bg-img-1"></div>
        <h2><a href="#">@Model.Titre</a></h2>
        <blockquote>
            <p class="location"><span class="glyphicon glyphicon-map-marker"></span>@Model.CodePostal @Model.Ville, @Model.Pays</p>
            <p>@Model.DescCourte</p>
        </blockquote>
    </div>
</div>
